03B1,6), \u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF.\u03B4\u03C5\u03B1\u03B4\u03B9\u03BA\u03CC(\u03B1\u03BB\u03C6\u03B1,30)\r\n\r\n\\\\ \u03BC\u03B5\u03C4\u03B1\u03C4\u03C1\u03BF\u03C0\u03AE \u03B5\u03BD\u03CC\u03C2 \r\n\u0391=\u03B4\u03C5\u03B1\u03B4\u03B9\u03BA\u03CC.\u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF(-500)\r\n\\\\ \u03B1\u03BD\u03AC\u03C0\u03BF\u03B4\u03B7 \u03BC\u03B5\u03C4\u03B1\u03C4\u03C1\u03BF\u03C0\u03AE\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF.\u03B4\u03C5\u03B1\u03B4\u03B9\u03BA\u03CC(\u0391)\r\n\\\\ \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1 \u03BA\u03C1\u03B1\u03C4\u03C1\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5 \u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF \u03C7\u03C9\u03C1\u03AF\u03C2 \u03C0\u03C1\u03CC\u03C3\u03B7\u03BC\u03BF \u03C3\u03B5 \u03BC\u03B1\u03BA\u03C1\u03CD.\r\n\u03BC\u03B1\u03BA\u03C1\u03CD\u03C2 B=\u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF.\u03B4\u03C5\u03B1\u03B4\u03B9\u03BA\u03CC(3405435354)\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u03B4\u03C5\u03B1\u03B4\u03B9\u03BA\u03CC.\u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF(B)\r\n\\\\ \u03BC\u03C0\u03BF\u03C1\u03BF\u03CD\u03BC\u03B5 \u03BD\u03B1 \u03C7\u03C1\u03B7\u03C3\u03B9\u03BC\u03B9\u03BF\u03C0\u03BF\u03B9\u03AE\u03C3\u03BF\u03C5\u03BC\u03B5 \u03C4\u03B9\u03C2 \u03BA\u03B1\u03BD\u03BF\u03BD\u03B9\u03BA\u03AD\u03C2 \u03BC\u03B5\u03C4\u03B1\u03B2\u03BB\u03B7\u03C4\u03AD\u03C2\r\n\\\\ \u03C9\u03C2 8 \u03C8\u03B7\u03C6\u03AF\u03B1 (byte) \u03C3\u03C4\u03B7 \u03BC\u03BD\u03AE\u03BC\u03B7\r\nAA=3405435354   \\\\\r\n\u0391%=3405435354  \\\\ \u03B5\u03C3\u03C9\u03C4\u03B5\u03C1\u03B9\u03BA\u03AC \u03B5\u03AF\u03BD\u03B1\u03B9 \u03B4\u03B9\u03C0\u03BB\u03CC\u03C2 (\u03BA\u03B9\u03BD\u03B7\u03C4\u03AE\u03C2 \u03C5\u03C0\u03BF\u03B4\u03B9\u03B1\u03C3\u03C4\u03BF\u03BB\u03AE\u03C2 \u03BC\u03B5 8 \u03C8\u03B7\u03C6\u03AF\u03B1)\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 AA, \u0391%\r\n\u0395\u03C0\u03B9\u03C3\u03C4\u03C1\u03BF\u03C6\u03AE \u03B1\u03BB\u03C6\u03B1, 10 :=AA+1 \u03C9\u03C2 \u03B4\u03B9\u03C0\u03BB\u03CC\u03C2  \\\\ 8 bytes - offset 10*4=+40\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u03B5\u03BA\u03C6\u03C1(\u03B1\u03BB\u03C6\u03B1, 40 \u03C9\u03C2 \u03B4\u03B9\u03C0\u03BB\u03CC\u03C2)  \\\\ offset  40 byte \r\n\u0394\u03B9\u03AC\u03C1\u03B8\u03C1\u03C9\u03C3\u03B7 alfa1 \u03C9\u03C2 \u03B1\u03BA\u03AD\u03C1\u03B1\u03B9\u03BF\u03C2 * 100\r\n\u0395\u03C0\u03B9\u03C3\u03C4\u03C1\u03BF\u03C6\u03AE alfa1, 10 :=AA \u03C9\u03C2 \u03B4\u03B9\u03C0\u03BB\u03CC\u03C2  \\\\ 8 bytes - offset 10*2=+20\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u03B5\u03BA\u03C6\u03C1(alfa1, 20 \u03C9\u03C2 \u03B4\u03B9\u03C0\u03BB\u03CC\u03C2)  \\\\ offset  20 byte \r\n\r\n5) \u0395\u03BA\u03C4\u03AD\u03BB\u03B5\u03C3\u03B7 \u039A\u03CE\u03B4\u03B9\u03BA\u03B1 \u03B3\u03BB\u03CE\u03C3\u03C3\u03B1\u03C2 \u03BC\u03B7\u03C7\u03B1\u03BD\u03AE\u03C2\r\n\u0394\u03B5\u03AF\u03C4\u03B5 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1 \u03C3\u03C4\u03B7\u03BD \u0395\u03BA\u03C4\u03AD\u03BB\u03B5\u03C3\u03B7 \u03B3\u03B9\u03B1 \u03C0\u03B1\u03C1\u03AC\u03B4\u03B5\u03B9\u03B3\u03BC\u03B1 \u03C4\u03B7\u03C2 \u0394\u03B9\u03AC\u03C1\u03B8\u03C1\u03C9\u03C3\u03B7 \u039A\u03CE\u03B4\u03B9\u03BA\u03B1\r\n\r\n6) \u03A7\u03C1\u03AE\u03C3\u03B7 \u03BC\u03B5 \u03C4\u03B7\u03BD \u03B2\u03B9\u03B2\u03BB\u03B9\u03BF\u03B8\u03AE\u03BA\u03B7 \u039C\u03B1\u03B8\u03B7\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC\r\n\u0394\u03BF\u03BC\u03AE \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1 {\r\n          x \u03A9\u03C2 \u0394\u03B9\u03C0\u03BB\u03CC\u03C2\r\n          y \u03A9\u03C2 \u0394\u03B9\u03C0\u03BB\u03CC\u03C2\r\n          z \u03A9\u03C2 \u0394\u03B9\u03C0\u03BB\u03CC\u03C2\r\n}\r\n\u0394\u03BF\u03BC\u03AE \u0394\u03B9\u03B1\u03BD\u03CD\u03C3\u03BC\u03B1\u03C4\u03B1 {\r\n      Vec1 \u03A9\u03C2 \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1\r\n      Vec2 \u03A9\u03C2 \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1\r\n}\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u039C\u03AE\u03BA\u03BF\u03C2(\u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1), \u039C\u03AE\u03BA\u03BF\u03C2(\u0394\u03B9\u03B1\u03BD\u03CD\u03C3\u03BC\u03B1\u03C4\u03B1)\r\n\u039A=\u039A\u03AC\u03B8\u03B5(\u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1)\r\n\u0395\u03BD\u03CE \u039A {\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0395\u03BA\u03C6\u03C1$(\u039A, \u039A^)+\" \u03B8\u03AD\u03C3\u03B7 \u03C3\u03C4\u03B7 \"+\u0395\u03BA\u03C6\u03C1$(\u039A)\r\n}\r\n\u0394\u03B9\u03AC\u03C1\u03B8\u03C1\u03C9\u03C3\u03B7 \u039A\u03B5\u03BD\u03AE \u0386\u03BB\u03C6\u03B1 \u03C9\u03C2 \u0394\u03B9\u03B1\u03BD\u03CD\u03C3\u03BC\u03B1\u03C4\u03B1*10\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0), \u0386\u03BB\u03C6\u03B1(0,\"Vec1\"), \u0386\u03BB\u03C6\u03B1(0,\"Vec2\")\r\n\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0, \"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"X\")) , \u0386\u03BB\u03C6\u03B1(0,\"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"Y\")), \u0386\u03BB\u03C6\u03B1(0,\"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"Z\"))\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0, \"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"X\")!) , \u0386\u03BB\u03C6\u03B1(0,\"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"Y\")!), \u0386\u03BB\u03C6\u03B1(0,\"Vec1\", \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"Z\")!)\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0, \"Vec1\") \r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0, \"Vec1\"!)  ' \u03B1\u03C6\u03B1\u03B9\u03C1\u03BF\u03CD\u03BC\u03B5  \u03C4\u03B7 \u03C4\u03B9\u03BC\u03AE \u0386\u03BB\u03C6\u03B1(0), \u03B4\u03B7\u03BB\u03B1\u03B4\u03AE \u03B4\u03AF\u03BD\u03B5\u03B9 \u03BC\u03CC\u03BD\u03BF \u03B7 \u03B8\u03AD\u03C3\u03B7 \u03C3\u03B5 \u03C8\u03B7\u03C6\u03AF\u03B1 \u03B1\u03C0\u03CC \u03C4\u03B7\u03BD \u03B1\u03C1\u03C7\u03AE.\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(0!) ' \u03C0\u03C1\u03BF\u03C3\u03B8\u03AD\u03C4\u03BF\u03C5\u03BC\u03B5 \u03C8\u03B7\u03C6\u03AF\u03B1 (bytes) \u03C3\u03C4\u03BF \u0386\u03BB\u03C6\u03B1(0), \u03C0\u03C7 \u0386\u03BB\u03C6\u03B1(10!) \u03B4\u03AF\u03BD\u03B5\u03B9 \u0391\u03BB\u03C6\u03B1(0)+10 \u03C9\u03C2 \u03B8\u03AD\u03C3\u03B7 \u03BC\u03BD\u03AE\u03BC\u03B7\u03C2\r\n\r\n\u0394\u03B9\u03B1\u03BD=\u039B\u03AC\u03BC\u03B4\u03B1 \u0386\u03BB\u03C6\u03B1, \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1 (\u03BD, \u03B1$, \u03B2$) -> {\r\n      =\u0388\u03BA\u03C6\u03C1(\u0386\u03BB\u03C6\u03B1, \u0386\u03BB\u03C6\u03B1(0, \u03B1$, \u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\u03B2$)!) \u03A9\u03C2 \u0394\u03B9\u03C0\u03BB\u03CC\u03C2)\r\n}\r\n\u038C\u03C1\u03B9\u03C3\u03B5 \u039C\u03B1\u03B8 \u039C\u03B1\u03B8\u03B7\u03BC\u03B1\u03C4\u03B9\u03BA\u03AC\r\n\u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0386\u03BB\u03C6\u03B1(1)-\u0386\u03BB\u03C6\u03B1(0)\r\nx_\u03B8\u03AD\u03C3\u03B7=\u0394\u03B9\u03AC\u03BD\u03C5\u03C3\u03BC\u03B1(\"x\")\r\nFor \u03BD=0 to 9 {\r\n      \u039C\u03AD\u03B8\u03BF\u03B4\u03BF\u03C2 \u039C\u03B1\u03B8, \"Vector\",\u0386\u03BB\u03C6\u03B1(\u03BD,\"Vec1\"), 1200,2500,1500\r\n      \u039C\u03AD\u03B8\u03BF\u03B4\u03BF\u03C2 \u039C\u03B1\u03B8, \"Vector\",\u0386\u03BB\u03C6\u03B1(\u03BD,\"Vec2\"), 3000,2000,1000\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0394\u03B9\u03B1\u03BD(\u03BD,\"Vec1\", \"x\")\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0394\u03B9\u03B1\u03BD(\u03BD,\"Vec1\", \"y\")\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0394\u03B9\u03B1\u03BD(\u03BD,\"Vec1\", \"z\")\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0388\u03BA\u03C6\u03C1(\u0386\u03BB\u03C6\u03B1, \u0386\u03BB\u03C6\u03B1(\u03BD, \"Vec1\", x_\u03B8\u03AD\u03C3\u03B7!) \u03A9\u03C2 \u0394\u03B9\u03C0\u03BB\u03CC\u03C2 )\r\n      \u03A4\u03CD\u03C0\u03C9\u03C3\u03B5 \u0394\u03B9\u03B1\u03BD(\u03BD,\"Vec2\", \"y\")\r\n     \u03A4