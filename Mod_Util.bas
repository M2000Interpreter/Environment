MergeOperators = A$
Else
br() = Split("[]" + A$ + "[]", "][")
For I = 1 To UBound(br) - 1
If InStr(b$, "[" + br(I) + "]") = 0 Then b$ = b$ + "[" + br(I) + "]"
Next I
MergeOperators = b$
End If
End Function
