Function StarSTR(ByVal sStr As String) As String
Dim L As Long, s As Long
L = Len(sStr)
sStr = RTrim$(sStr)
s = L - Len(sStr)
StarSTR = String$(L - s, "*") + String$(s, " ")

End Function
